{
  "name": "opacity-project-frontend",
  "version": "1.0.0",
  "description": "Front-end for opacity-project-api",
  "main": "index.js",
  "author": "akatsukijs",
  "license": "GPL-3.0-or-later",
  "sideEffects": false,
  "scripts": {
    "dev": "webpack-dev-server --hot --progress --open --config webpack.dev.js",
    "prod": "NODE_ENV='production' webpack --progress --config webpack.prod.js",
    "flow": "flow",
    "lint:fix": "standard --fix --parser babel-eslint --plugin flowtype \"src/**/*.{jsx,js}\"",
    "prettier": "prettier-standard \"src/**/*.{jsx,js}\" && yarn lint:fix",
    "storybook": "start-storybook -s ./assets -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook"
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "prettier-standard",
      "standard --fix --parser babel-eslint --plugin flowtype",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@storybook/addon-actions": "5.0.0-beta.2",
    "@storybook/addon-info": "5.0.0-beta.2",
    "@storybook/addon-knobs": "5.0.0-beta.2",
    "@storybook/addon-options": "5.0.0-beta.2",
    "@storybook/react": "5.0.0-beta.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.6",
    "eslint": "^5.13.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "flow-bin": "^0.92.0",
    "flow-webpack-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "mini-css-extract-plugin": "^0.5.0",
    "prettier": "^1.16.4",
    "prettier-standard": "^9.1.1",
    "standard": "^12.0.1",
    "standard-loader": "^6.0.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "emotion-theming": "^10.0.7",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-svg": "^7.2.7"
  }
}
